{
  "enableRecording": false,
  "enableOCR": false,
  "enableCloud": false,
  "cloud": {
    "provider": "minio",
    "endpoint": "https://minio.yourdomain.com",
    "bucket": "activity",
    "accessKey": "your-access-key",
    "secretKey": "your-secret-key",
    "region": "us-east-1",
    "encryptionKey": "your-encryption-key",
    "syncIntervalMin": 5
  },
  "recording": {
    "fps": 30,
    "bitrate": 1000000,
    "maxFileMin": 10,
    "quality": 0.8,
    "audioEnabled": true,
    "videoCodec": "h264"
  },
  "ocr": {
    "language": "zh-CN",
    "intervalSec": 5,
    "cropRatio": 0.3,
    "enableAutoLanguage": false
  },
  "paths": {
    "recordings": "%APPDATA%/activity-analytics/screen",
    "ocrCache": "%APPDATA%/activity-analytics/ocr",
    "database": "%APPDATA%/activity-analytics/data.db"
  }
}
    